table1 <- data.frame(
  Column1 = c(1, 2, 3),
  Column2 = c(4, 5, 6),
  Column3 = c(7, 8, 9),
  Column4 = c(10, 11, 12)
)
table2 <- data.frame(
  Column1 = c(13, 14, 15),
  Column2 = c(16, 17, 18),
  Column3 = c(19, 20, 21),
  Column4 = c(22, 23, 24)
)
print("Table 1:")
print(table1)
print("Table 2:")
print(table2)
array3d <- array(c(as.matrix(table1), as.matrix(table2)), dim = c(3, 4, 2))
print("Content of the 3D Array:")
print(array3d)
