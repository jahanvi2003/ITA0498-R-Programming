my_function <- function(x) {
  return(x^2)
}
vector1 <- c(1, 2, 3)
vector2 <- c("apple", "banana", "cherry")
matrix1 <- matrix(c(1, 2, 3, 4, 5, 6), nrow = 2, byrow = TRUE)
my_list <- list(
  Vector1 = vector1,
  Vector2 = vector2,
  Matrix1 = matrix1,
  Function = my_function
)
print(my_list)
print(my_list$Function(5))
